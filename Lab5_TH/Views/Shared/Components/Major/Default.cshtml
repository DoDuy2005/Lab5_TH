@model IEnumerable<dynamic>

<div class="card">
    <div class="card-header bg-light">
        <h6 class="card-title mb-0">
            <i class="fas fa-filter me-1"></i>Filter by Major
        </h6>
    </div>
    <div class="list-group list-group-flush">
        @foreach (var major in Model)
        {
            <a href="#" class="list-group-item list-group-item-action major-filter" 
               data-major-id="@major.MajorID">
                @major.MajorName
            </a>
        }
    </div>
</div>